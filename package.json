{
  "name": "test_app",
  "version": "1.0.0",
  "description": "Yet another app",
  "main": "build/server.js",
  "scripts": {
    "precommit": "lint-staged",
    "clean": "rimraf public build",
    "start": "better-npm-run dev",
    "lint": "eslint 'src/**' 'webpack/**' --ext .js --cache --fix --ignore-path .eslintignore"
  },
  "betterScripts": {
    "dev": {
      "command": "npm run clean && node src/server/index.js",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "prod": {
      "command": "node src/server/index.js",
      "env": {
        "NODE_ENV": "production"
      }
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --cache --fix --ignore-path .eslintignore",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Guru107/test_app.git"
  },
  "keywords": [
    "preact",
    "pwa",
    "service workers"
  ],
  "author": "Guru107 <guruak107@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Guru107/test_app/issues"
  },
  "homepage": "https://github.com/Guru107/test_app#readme",
  "devDependencies": {
    "babel-core": "6.26.0",
    "babel-eslint": "7.2.3",
    "babel-loader": "7.1.2",
    "babel-plugin-dual-import": "1.1.5",
    "babel-plugin-module-resolver": "2.7.1",
    "babel-plugin-transform-react-jsx": "6.24.1",
    "babel-preset-env": "1.6.0",
    "babel-preset-es2015": "6.24.1",
    "better-npm-run": "0.1.0",
    "css-loader": "0.28.5",
    "eslint": "4.5.0",
    "eslint-config-prettier": "2.3.0",
    "eslint-config-standard": "10.2.1",
    "eslint-config-standard-preact": "1.1.6",
    "eslint-plugin-import": "2.7.0",
    "eslint-plugin-node": "5.1.1",
    "eslint-plugin-prettier": "2.2.0",
    "eslint-plugin-promise": "3.5.0",
    "eslint-plugin-react": "7.3.0",
    "eslint-plugin-standard": "3.0.1",
    "extract-css-chunks-webpack-plugin": "2.0.16",
    "husky": "0.14.3",
    "less": "2.7.2",
    "less-loader": "4.0.5",
    "lint-staged": "4.0.4",
    "postcss-loader": "2.0.6",
    "prettier": "1.5.3",
    "react-hot-loader": "3.0.0-beta.7",
    "rimraf": "2.6.1",
    "style-loader": "0.18.2",
    "webpack": "3.5.5",
    "webpack-dev-middleware": "1.12.0",
    "webpack-dev-server": "2.7.1",
    "webpack-hot-middleware": "2.18.2",
    "webpack-hot-server-middleware": "0.1.0",
    "webpack-node-externals": "1.6.0",
    "write-file-webpack-plugin": "4.1.0"
  },
  "dependencies": {
    "babel-plugin-universal-import": "1.2.7",
    "express": "4.15.4",
    "express-no-favicons": "0.0.1",
    "lodash": "4.17.4",
    "material-design-lite": "1.3.0",
    "preact": "8.2.4",
    "preact-compat": "3.17.0",
    "preact-mdl": "2.2.1",
    "preact-router": "2.5.7",

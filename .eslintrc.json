{
	"parser": "babel-eslint",
	"extends": ["eslint:recommended", "plugin:react/all","prettier"],
	"plugins":["react","prettier"],
	"parserOptions": {
		"ecmaFeatures": {
		  "jsx": true
		}
	  },
	"rules": {
		"prettier/prettier":["error",{
			"tabWidth":4,
			"useTabs":true,
			"semi":false
		}]
	}
}